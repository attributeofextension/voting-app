<div class="container-fluid middle myPolls">
    <h2>These are my polls</h2>
   
   <div class="container myPollsPanel">
    <div class="card">
      <ul class="list-group list-group-flush">
               
        {{#unless polls}}<li class="list-group-item">There are currently no polls</li>{{/unless}}
        {{#each polls}}
        <li class="list-group-item">
        <a href="{{url}}">{{name}}</a> by {{userid}}
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>